    "morgan": "^1.9.1",
    "multer": "^1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "notenv": "^2.0.3",
    "open-graph": "^0.2.6",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.5.1",
    "sequelize-cli": "^6.2.0",
    "stream-chat": "^3.13.1",
    "swagger-ui-express": "^4.1.6",
    "url": "^0.11.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@shelf/jest-mongodb": "^4.1.7",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-prettier": "^4.2.1",
    "generate-changelog": "^1.8.0",
    "husky": "^7.0.4",
    "ioredis-mock": "^7.5.0",
    "jest": "^29.7.0",
    "lint-staged": "^13.2.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.8.0",
    "supertest": "^6.3.3"
  },
  "lint-staged": {
    "./**/*.{js,ts,jsx,tsx}": [
      "yarn eslint --cache --fix"
    ],
    "*.json": [
      "prettier --write"
    ]
  }
