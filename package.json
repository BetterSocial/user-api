    "axios": "^0.21.1",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.19.0",
    "bull": "^4.1.4",
    "cli": "^1.0.1",
    "cloudinary": "^1.25.0",
    "cookie-parser": "^1.4.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "elastic-builder": "^2.16.0",
    "elasticsearch": ">=1.1.0",
    "emoji-regex": "^10.1.0",
    "emoji-unicode": "^2.0.1",
    "express": "^4.16.4",
    "express-limiter": "^1.6.1",
    "fastest-validator": "^1.10.0",
    "firebase-admin": "^11.5.0",
